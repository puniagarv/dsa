//{ Driver Code Starts

#include<bits/stdc++.h>
using namespace std;

// } Driver Code Ends

class Solution {
public:
	int sumOfPowers(int a, int b)
	{
	    // Code here
        
        vector<int>v(b+1,1);
        
        v[1]=0,v[2]=1;
        for(int i=2;i*i<=b;i++)
        
        {
              if(v[i]==1)
              {
              	for(int j=2*i;j<=b;j+=i)
              		v[j]=0;
              }
        }
         
         
        int ans=0;
        
        for(int i=a;i<=b;i++)
        {
        	if(v[i]==0)
        	{  
        		int n=i;
        
               for(int j=2;j*j<=n;j++)
               {
                	while(n%j==0)
                	{
               		 ans++;
               		  n=n/j;
                	}
        
               }
               if(n > 1)
               	ans++;
        
        	}
        	else
        		ans++;
        
        }
        return ans;
        	    
    }
};

//{ Driver Code Starts.
int main(){
	int tc;
	cin >> tc;
	while(tc--){
		int a, b;
		cin >>  a >> b;
		Solution obj;
		int ans = obj.sumOfPowers(a, b);
		cout << ans <<"\n";
	}
	return 0;
}
// } Driver Code Ends
